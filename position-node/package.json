{
  "name": "position-node",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "ts-node src/worker.ts",
    "start.watch": "nodemon src/worker.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "devDependencies": {
    "@temporalio/nyc-test-coverage": "^1.4.2",
    "@temporalio/testing": "^1.4.2",
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^29.1.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "jest": "^29.1.2",
    "nodemon": "^2.0.14",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@temporalio/client": "^1.4.2",
    "@temporalio/worker": "^1.4.2",
    "@temporalio/workflow": "^1.4.2"
  }
}
